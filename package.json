{
  "name": "zen-hordes",
  "version": "1.11.0",
  "description": "An extension that enhances the user experience of the MyHordes website.",
  "main": "index.js",
  "scripts": {
    "watch": "webpack --config webpack/webpack.dev.js --watch",
    "build:chrome": "sh ./scripts/build.sh chrome",
    "build:firefox": "sh ./scripts/build.sh firefox",
    "source": "sh ./scripts/source.sh",
    "clean": "rimraf dist",
    "style": "prettier --write \"src/**/*.{ts,tsx}\"",
    "preinstall": "npx only-allow pnpm",
    "data:generate": "tsx ./scripts/generateData.ts",
    "mh-api:get-types": "sh ./scripts/getMhApiTypes.sh"
  },
  "author": "Zenoo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zenoo/zen-hordes.git"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.2.5",
    "@mui/icons-material": "^6.4.6",
    "@mui/material": "^6.4.6",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/chrome": "0.0.158",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.10",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "bestzip": "^2.2.1",
    "copy-webpack-plugin": "^9.0.1",
    "dotenv": "^16.5.0",
    "eslint": "^9.22.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "glob": "^11.0.1",
    "jsdom": "^26.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^6.0.1",
    "ts-loader": "^8.0.0",
    "tsx": "^4.19.3",
    "typescript": "^5.0.4",
    "typescript-eslint": "^8.26.1",
    "webpack": "^5.94.0",
    "webpack-cli": "^4.0.0",
    "webpack-merge": "^5.0.0",
    "yaml": "^2.8.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}